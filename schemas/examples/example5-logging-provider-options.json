{
  "$schema": "../appsettings.schema.json",
  "Logging": {
    "LogLevel": {
      "Default": "Debug"
    },
    "NLog": {
      "IncludeScopes": true,
      "CaptureMessageProperties": true,
      "CaptureMessageTemplates": false,
      "EventIdSeparator": "-",
      "IgnoreEmptyEventId": true,
      "ParseMessageTemplates": true,
      "ShutdownOnDispose": true
    }
  },
  "NLog": {
    "autoReload": true
  }
}