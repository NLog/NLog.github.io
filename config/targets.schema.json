{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://nlog-project.org/targets.json",
    "type": "array",
    "items": {
        "$id": "#/items",
        "type": "object",
        "required": [
            "name",
            "page",
            "description",
            "category"
        ],
        "properties": {
            "name": {
                "$id": "#/items/properties/name",
                "type": "string",
                "default": "",
                "examples": [
                    "Chainsaw"
                ],
                "pattern": "^(.*)$"
            },
            "page": {
                "$id": "#/items/properties/page",
                "type": "string",
                "default": "",
                "examples": [
                    "Database-target",
                    "https://mypage/example.html"
                ],
                "description": "Full URL or NLog wiki page",
                "pattern": "^(.*)$"
            },
            "description": {
                "$id": "#/items/properties/description",
                "type": "string",
                "default": "",
                "pattern": "^(.*)$"
            },
            "platforms": {
                "$id": "#/items/properties/platforms",
                "type": "array",
                "items": {
                    "$id": "#/items/properties/platforms/items",
                    "type": "string",
                    "enum": [
                        "net35",
                        "net40",
                        "net45",
                        "netstandard1.3",
                        "netstandard1.5",
                        "netstandard2.0",
                        "ios",
                        "android",
                        "mono",
                        "wp8",
                        "sl"
                    ]
                }
            },
            "keywords": {
                "$id": "#/items/properties/keywords",
                "type": "array",
                "items": {
                    "$id": "#/items/properties/keywords/items",
                    "type": "string",
                    "default": "",
                    "examples": [
                        "web",
                        "http"
                    ],
                    "pattern": "^(.*)$"
                }
            },
            "category": {
                "$id": "#/items/properties/category",
                "type": "string",
                "default": "",
                "enum": [
                    "Integrations (GUI tools)",
                    "",
                    "Databases",
                    "Programmatically",
                    "Integrations",
                    "Webservices and networking",
                    "Async and buffering",
                    "Filtering, throttling and error handling",
                    "Security",
                    "Windows Forms",
                    "Integrations (Cloud)"
                ]
            },
            "aliases": {
                "$id": "#/items/properties/aliases",
                "type": "array",
                "items": {
                    "$id": "#/items/properties/aliases/items",
                    "type": "string",
                    "default": ""
                }
            },
            "wrapper": {
                "type": "boolean",
                "description": "Wraps another target"
            },
            "external": {
                "type": "boolean",
                "description": "Not maintainted by the NLog team"
            },
            "package": {
                "type": "string",
                "examples": [
                    "NLog.Web"
                ]
            }
        }
    }
}